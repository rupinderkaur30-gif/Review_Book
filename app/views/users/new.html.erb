<%= form_for @user do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <%= f.label :password_confirmation %>
    <%= f.password_field :password_confirmation %>
     <%= f.label :age %>
    <%= f.number_field :age %>
    <%= f.submit "sign_up"%>
<% end %>